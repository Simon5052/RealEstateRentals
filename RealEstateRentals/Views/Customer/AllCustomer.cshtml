
@{
    ViewData["Title"] = "AllCustomers";
    var customers = ViewBag.Customers;
}

<h1>AllCustomers</h1>


<div>

      <table class="table">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
           
        </tr>



        @foreach (var customer in customers)
        {
        <tr>
            <td>@customer.FirstName</td>
            <td>@customer.LastName</td>
            <td>@customer.Email</td>
            <td>@customer.PhoneNumber</td>


        </tr>
        }
    </table>








</div>


